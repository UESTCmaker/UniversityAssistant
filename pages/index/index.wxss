.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.head-text {
  display: flex;
  flex-direction: column;
  text-align: center;
}

.head-text text {
  margin-top: 20rpx;
  font-size: 40rpx;
}

.search {
  display: flex;
  flex-direction: row;
  margin-top: 100rpx;
  width: 500rpx;
  padding: 20rpx;
  border: 2px solid #6a7d8f;
  border-radius: 30rpx;
}
.string{
  width: 500rpx;
  font-size: 40rpx;
  margin: auto 0 auto 20rpx;
}

.inputString {
  font-size: 40rpx;
}

.searchbutton {
  width: 300rpx;
  margin-top: 50rpx;
  background-color: #6a7d8f;
  color: white;
  border-radius: 30rpx;
  letter-spacing: 2px;
}
